{
  "name": "FSY",
  "manifest_version": 3,
  "version": "0.1",
  "description": "Turns the page red when you click the icon",
  "permissions": [
    "storage",
    "notifications",
    "scripting",
    "*://*.google.com/"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "300": "./icon/origin_on.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
  ],
  "action": {
    "default_icon": "./icon/origin_on.png",
    "default_popup": "./popup/popup.html"
  }
}